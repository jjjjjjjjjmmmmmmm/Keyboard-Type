    if (!document.body.hasAttribute('tabindex')) document.body.setAttribute('tabindex', '0');
    window.addEventListener('load', () => document.body.focus());
    
    // ---- Key-code normalizer ----
    function codeToKey(code) {
      // Letters: KeyA → "a"
      if (/^Key[A-Z]$/.test(code)) return code.slice(3).toLowerCase();
    
      // Numbers: Digit1 → "1"
      if (/^Digit[0-9]$/.test(code)) return code.slice(5);
    
      // F-keys: F1 → "f1"
      if (/^F[0-9]{1,2}$/.test(code)) return code.toLowerCase();
    
      // map punctuation / special keys to keyMap names
      const map = {
        Space: ' ',
        Minus: '-',
        Equal: '=',
        BracketLeft: '[',
        BracketRight: ']',
        Semicolon: ';',
        Quote: "'",
        Backslash: '\\',
        Comma: ',',
        Period: '.',
        Slash: '/',
        Escape: 'escape',
        Tab: 'tab',
        CapsLock: 'capslock',
        Enter: 'enter',
        Backspace: 'backspace',
        ShiftLeft: 'shiftleft',
        ShiftRight: 'shiftright',
        ControlLeft: 'control',
        ControlRight: 'controlright',
        AltLeft: 'altleft',
        AltRight: 'altright',
        MetaLeft: 'metaleft',
        MetaRight: 'metaright',
        ArrowLeft: 'arrowleft',
        ArrowRight: 'arrowright',
        ArrowUp: 'arrowup',
        ArrowDown: 'arrowdown',
        ContextMenu: 'contextmenu'
      };
    
      return map[code] || code.toLowerCase();
    }
    
    document.addEventListener('keydown', e => {
        // prevent F-keys, Tab, etc. from triggering browser actions if you wish
        if (e.code === 'F12') e.preventDefault();
        if (e.code === 'Tab' || e.code === 'Backspace') e.preventDefault();
      
        let normalized = codeToKey(e.code);
      
        // --- Shift-combo fallbacks for missing keys ---
        if (e.shiftKey) {
          switch (e.key.toLowerCase()) {
            case 'q': normalized = '§'; break;          
            case 'p': normalized = 'power'; break;      
            case 'o': normalized = 'f11'; break;        
            case 'f': normalized = 'fn'; break;         
            case 'z': normalized = '`'; break;  
          }
        }
      
        console.log('keydown ->', { code: e.code, key: e.key, normalized });
      
        const mappedId = keyMap[normalized];
        if (!mappedId) return;
      
        const el = document.getElementById(mappedId);
        if (!el) return;
      
        const isVisible = window.getComputedStyle(el).opacity === '1';
        el.style.opacity = isVisible ? '0.05' : '1';
      });
    </script>

</svg>
<div id="instructions-container" style="
  position: fixed;
  top: 20px;
  left: 20px;
  background: #f9f8f5;
  color: #181818;
  border-radius: 16px;
  overflow: hidden;
  width: 450px;
  transition: all 0.4s ease;
  max-height: 400px;
">
  <div id="instructions-header" style="
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10px 16px;
    cursor: pointer;
  ">
    <span style="font-size: 16px; font-weight: 600;">Instructions</span>
    <span id="chevron" style="
      display: inline-block;
      transition: transform 0.3s ease;
      transform: rotate(270deg);
      font-size: 18px;
      user-select: none;
    ">&lt;</span>
  </div>

  <div id="instructions-content" style="
    opacity: 0;
    height: 0;
    padding: 0 16px;
    font-size: 15px;
    line-height: 2;
    overflow: hidden;
    transition: all 0.4s ease;
  ">
  Press keyboard key you would like to expose, press again to hide.<br><br>

  <strong>Non-Functioning key combinations:</strong><br>
  fn = Shift + F<br>
  § = Shift + Q<br>
  ` = Shift + Z<br>
  ⏻ = Shift + P<br>
  F11 = Shift + O<br><br>

  F1–F10 = fn + F1–F10<br>
  F12 = fn + F12<br><br>

  To output letterform press <strong>Download SVG</strong>.
  </div>
</div>

<script>
  const header = document.getElementById('instructions-header');
  const content = document.getElementById('instructions-content');
  const container = document.getElementById('instructions-container');
  const chevron = document.getElementById('chevron');

  header.addEventListener('click', () => {
    const expanded = container.classList.toggle('expanded');

    if (expanded) {
      content.style.height = content.scrollHeight + 'px';
      content.style.opacity = '1';
      container.style.maxHeight = (content.scrollHeight + 80) + 'px';
      chevron.style.transform = 'rotate(90deg)';
    } else {
      content.style.height = '0';
      content.style.opacity = '0';
      container.style.maxHeight = '50px';
      chevron.style.transform = 'rotate(270deg)';
    }
  });
</script>


     
     <button id="download-svg" style="
  position: fixed;
  bottom: 20px;
  right: 20px;
  padding: 10px 18px;
  font-family: 'Kosugi', sans-serif;
  font-size: 16px;
  border-radius: 16px;
  border: none;
  background: #f9f8f5;
  color: rgb(24, 24, 24);
  cursor: pointer;">
  <strong>Download SVG</strong>
  </button>

  <script>
    document.getElementById('download-svg').addEventListener('click', function () {
        const svgElement = document.getElementById('keyboard');
        if (!svgElement) return;
    

        const clone = document.createElementNS("http://www.w3.org/2000/svg", "svg");
        clone.setAttribute("xmlns", "http://www.w3.org/2000/svg");
        clone.setAttribute("width", svgElement.getAttribute("width"));
        clone.setAttribute("height", svgElement.getAttribute("height"));
        clone.setAttribute("viewBox", svgElement.getAttribute("viewBox"));
    

        svgElement.querySelectorAll('[id]').forEach(el => {
            const opacity = window.getComputedStyle(el).opacity;
            if (opacity === '1') {
                clone.appendChild(el.cloneNode(true));
            }
        });
    

        let source = new XMLSerializer().serializeToString(clone);
        source = '<?xml version="1.0" encoding="UTF-8"?>\n' + source;
    
    
        const blob = new Blob([source], { type: 'image/svg+xml;charset=utf-8' });
        const url = URL.createObjectURL(blob);
    
        const a = document.createElement('a');
        a.href = url;
        a.download = 'keyboardtypelayout.svg';
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
    });
    </script>
    
    
      
      </body>
      </html>
